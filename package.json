  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "ele": "electron .",
    "dev": "concurrently -k \"wait-on http://localhost:3000 && electron .\" \"cross-env BROWSER=none npm start\"",
